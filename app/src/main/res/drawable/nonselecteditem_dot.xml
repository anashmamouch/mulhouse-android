<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <size android:height="@dimen/outer_shape" android:width="@dimen/outer_shape"/>
        </shape>
    </item>
    <item android:right="@dimen/inner_shape_margin1" android:left="@dimen/inner_shape_margin1" android:bottom="@dimen/inner_shape_margin1"
        android:top="@dimen/inner_shape_margin1">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <size android:height="@dimen/inner_shape1" android:width="@dimen/inner_shape1"/>
            <solid android:color="@color/bck_layout"/>
            <stroke
                android:width="2dp"
                android:color="@color/colorPrimary" />
        </shape>
    </item>
</layer-list>