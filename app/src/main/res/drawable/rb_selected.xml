<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <size android:height="@dimen/rb_size" android:width="@dimen/rb_size"/>
            <stroke
                android:width="2dp"
                android:color="@color/colorPrimary" />

        </shape>
    </item>
    <item android:right="@dimen/rb_margin" android:left="@dimen/rb_margin" android:bottom="@dimen/rb_margin"
        android:top="@dimen/rb_margin">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <size android:height="@dimen/rb_size2" android:width="@dimen/rb_size2"/>
            <solid android:color="@color/colorPrimary"/>
        </shape>
    </item>
</layer-list>
